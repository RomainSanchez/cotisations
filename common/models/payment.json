{
  "name": "Payment",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "foreignKeys": {},
  "properties": {
    "date": {
      "type": "string",
      "required": true
    },
    "label": {
      "type": "string",
      "required": true
    },
    "debit": {
      "type": "string"
    },
    "credit": {
      "type": "string"
    },
    "valueDate": {
      "type": "string"
    },
    "createdAt": {
      "type": "date",
      "defaultFn": "now"
    },
    "disbursedAt": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "import": {
      "type": "belongsTo",
      "model": "Import",
      "foreignKey": "importId",
      "options": {
        "nestRemoting": true
      }
    },
    "debts": {
      "type": "hasAndBelongsToMany",
      "model": "Debt",
      "throughTable": "match",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
